syntax =  "v1"


@server (
    prefix: /api/cache
    group:  testcache
    middleware: TestCacheMiddleware
)
service demo-api {
    @handler set
    post /set returns (Response)
    
    @handler get
    post /get returns (Response)


    @handler kafkaSet
    post /kafka/set returns (Response)
    
    @handler kafkaGet
    post /kafka/get returns (Response)
    
}
